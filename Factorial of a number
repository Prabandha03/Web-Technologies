 <body>	
     <script>
	var a= parseInt(prompt("Enter Number"));
	document.write(factorial(a));
	function factorial(n)
	{
		if (n<=1){
		    return 1;}
		else{
		    return n*fact(n-1);}
	}
      </script>
 </body>
