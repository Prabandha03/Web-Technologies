<script>
function addition(a,b,c)
{
 for (let i=0;i<n;i++){
     for(let j=0;j<n;j++){
	c[i][j]=a[i][j]+b[i][j];
	} }
}
let n=3;
let a=[[1,2,3],[4,5,6],[7,8,9]];
let b=[[1,0,0],[0,1,0],[0,0,1]];
let c=new Array(n);
for ( var i=0;i<n;i++)
{
c[i]=new Array(n);
}
addition(a,b,c);
document.write('The resultant matrix is:'+'<br>');
for(let i=0;i<n;i++)
{
for(let j=0;j<n;j++)
{
document.write(c[i][j]+' ');
}
document.write('<br>');
}
</script>
