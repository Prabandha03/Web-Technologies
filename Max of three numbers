<script>
var a = parseInt(prompt("Enter a:"));
var b = parseInt(prompt("Enter b:"));
var c = parseInt(prompt("Enter c:"));
if(a>b && a>c){
document.write(a+" is biggest number");}
else if (b>c){
document.write(b+" is biggest number");}
else document.write(c+" is biggest number");
</script>
